{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@core/*": [
        "src/app/core/*"
      ],
      "@core-service/*": [
        "src/app/core/services/*"
      ],
      "@home/*": [
        "src/app/feature/home/*"
      ],
      "@producto/*": [
        "src/app/feature/producto/*"
      ],
      "@tarifa/*": [
        "src/app/feature/tarifa/*"
      ],      
      "@propietario/*": [
        "src/app/feature/propietario/*"
      ]      ,
      "@inmueble/*": [
        "src/app/feature/inmueble/*"
      ],
      "@pagoimpuestopredial/*": [
        "src/app/feature/pagoimpuestopredial/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ]      
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}